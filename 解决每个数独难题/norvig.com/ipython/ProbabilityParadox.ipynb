    "deletable": true,
    "new_sheet": false,
    "run_control": {
     "read_only": false
    }
   },
   "outputs": [],
   "source": [
    "monty2 = Uniform({'Car1/Pick1/Open2/Goat', 'Car1/Pick1/Open3/Goat',\n",
    "                  'Car2/Pick1/Open2/Car',  'Car2/Pick1/Open3/Goat',\n",
    "                  'Car3/Pick1/Open2/Goat', 'Car3/Pick1/Open3/Car'})"
   ]
