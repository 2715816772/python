   "cell_type": "code",
   "execution_count": 33,
   "metadata": {
    "button": false,
    "collapsed": true,
    "deletable": true,
    "new_sheet": false,
    "run_control": {
     "read_only": false
    }
   },
   "outputs": [],
   "source": [
    "monty = Uniform({'Car1/Lo/Pick1/Open2', 'Car1/Hi/Pick1/Open3',\n",
    "                 'Car2/Lo/Pick1/Open3', 'Car2/Hi/Pick1/Open3',\n",
    "                 'Car3/Lo/Pick1/Open2', 'Car3/Hi/Pick1/Open2'})"
   ]
