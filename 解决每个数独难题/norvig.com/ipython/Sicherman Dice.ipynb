   },
   "outputs": [],
   "source": [
    "def lower_bounds(N):\n",
    "    \"A list of lower bounds for respective sides of an N-sided die.\"\n",
    "    lowers = [1]\n",
    "    for _ in range(N-1):\n",
    "        m = lowers[-1] # The last number in lowers so far\n",
    "        lowers.append(m if (lowers.count(m) < m) else m + 1)\n",
    "    lowers[-1] = lowers[-2] + 1\n",
    "    return lowers"
   ]
  },
