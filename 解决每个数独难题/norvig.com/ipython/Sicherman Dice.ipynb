   "outputs": [],
   "source": [
    "def all_dice():\n",
    "    \"A list of all feasible 6-sided dice for the Sicherman problem.\"\n",
    "    return [(1, s2, s3, s4, s5, s6)\n",
    "            for s2 in ints(2, 7) \n",
    "            for s3 in ints(s2, 7)\n",
    "            for s4 in ints(max(s3, 3), 7)\n",
    "            for s5 in ints(s4, 7) \n",
    "            for s6 in ints(s5+1, 8)]"
   ]
  },
